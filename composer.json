{
  "name": "blueways/bw-static-template",
  "type": "typo3-cms-extension",
  "description": "TYPO3 extension that adds a frontend plugin for rendering static fluid templates. Inject JSON data into the templates.",
  "homepage": "https://blueways.de",
  "license": "MIT",
  "keywords": [
    "TYPO3"
  ],
  "require": {
    "blueways/bw-jsoneditor": "^1.0",
    "typo3/cms-core": "^9.0 || ^10.0 || ^11.0"
  },
  "require-dev": {
    "typo3/cms-base-distribution": "^11.0"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "public",
      "extension-key": "bw_static_template"
    }
  },
  "config": {
    "bin-dir": "bin",
    "vendor-dir": "vendor",
    "platform": {
      "php": "7.4"
    }
  },
  "autoload": {
    "psr-4": {
      "Blueways\\BwStaticTemplate\\": "Classes/"
    }
  }
}
